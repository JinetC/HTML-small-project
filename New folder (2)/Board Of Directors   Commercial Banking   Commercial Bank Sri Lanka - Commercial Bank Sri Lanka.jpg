<script>
    window.onload = function() {
        var detective = new Detector();
        //console.log(detective.detect('Open Sans'));
        if(!detective.detect('Open Sans')){
            $('<link rel="stylesheet" type="text/css" href="/newweb/templates/main/styles/open-sans.css" />').appendTo('head');
        }
    };

    var is_safari = navigator.userAgent.indexOf("Safari") > -1;
    var is_chrome = navigator.userAgent.indexOf('Chrome') > -1;

    $.fn.ripple = function (options) {
        var container;
        var properties = {
            color: 'grey'
        };
        $.extend(properties, options);
        $(this).mousedown(function (e) {
            container = $(this);
            var size, x, y;
            container.css({
                overflow: 'hidden',
                position: 'relative'
            });
            if (container.find('.ink').length === 0) {
                container.prepend("<span class='ink'></span>");
            }
            var ink = container.find('.ink');
            ink.removeClass('animated');
            if (!ink.height() && !ink.width()) {
                size = Math.max(container.outerWidth(), container.outerHeight());
                ink.css({background: properties.color, height: size, width: size});
            }
            x = e.pageX - container.offset().left - ink.width() / 2;
            y = e.pageY - container.offset().top - ink.height() / 2;
            ink.css({
                top: y,
                left: x,
            }).addClass('animated').addClass('pressed');
        });
        $(this).mouseup(function () {
            container = $(this);
            container.find('.ink').removeClass('pressed');
        });
    };

    $(function () {
        $('.text_area_slider').addClass('run');


        if (navigator.userAgent.indexOf('Safari') != -1 && navigator.userAgent.indexOf('Chrome') == -1) {
            $('body').addClass('safari');
        }
        if (navigator.userAgent.toLowerCase().indexOf('firefox') > -1) {
            $('body').addClass('safari');
        }

        $('.side_linkers ul li > a').ripple({color: '#ffffff'});
        $('.sideBar ul a').ripple({color: '#e2e2e2'});
        $('.right_top_header ul li a').ripple({color: '#43a2e8'});

        if ($(window).width() < 1000) {
            $('.mid_left ul li').removeClass('active ');
            $('.sub_menu_items').removeClass('activate ');
        }

        sidebar_height = $('.sideBar').height();
        banner_height = $('.inner_page_banner').height();
        $('.cover_eliments').css('min-height', sidebar_height + banner_height + 120);


        var $sidebar = $(".sideBar"),
            $window = $(window),
            offset = $sidebar.offset(),
            topPadding = 20;
        $sidebar.css('margin-top', -20);
        get_height = $sidebar.height() / 1;
        $window.scroll(function () {
            if ($(window).scrollTop() + $(window).height() > $(document).height() - get_height) {
                //console.log('kasun');
            } else {
                if ($window.scrollTop() > offset.top) {
                    $sidebar.stop().animate({
                        marginTop: $window.scrollTop() - offset.top + topPadding
                    });
                    $sidebar.addClass('activate');
                    $('.hidden_title').slideDown("slow");
                } else {
                    $sidebar.stop().animate({
                        marginTop: -20
                    });
                    $sidebar.removeClass('activate');
                    $('.hidden_title').slideUp("fast");
                }
            }

        });

        // Auto search
        $('.chzn-done').chosen();
        $(".item_identity").click(function () {
            $('#favourites').toggleClass('actives');
        });

    });
    function hidehome() {
        $('.bookmark_bar').hide();
    }
</script>
